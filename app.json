{
  "expo": {
    "name": "Workly",
    "slug": "Workly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": [
      "workly"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hanif.workly",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#4A90E2"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hanif_jamadar.Workly"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
   "plugins": [
  "expo-router",
  [
    "expo-location",
    {
      "locationAlwaysAndWhenInUsePermission": "Allow app to use your location."
    }
  ],
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/splash.png",
      "imageWidth": 200,
      "resizeMode": "cover",
      "backgroundColor": "#4A90E2"
    }
  ],
  "expo-font"
],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "persistNavigation": true
    },
      "eas": {
        "projectId": "5aabec1d-0105-4209-bf1f-eb7156952414"
      }
    },
    "owner": "hanif_jamadar",
    "runtimeVersion": "1.0.0",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5aabec1d-0105-4209-bf1f-eb7156952414"
    }
  }
}
